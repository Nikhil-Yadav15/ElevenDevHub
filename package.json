{
  "name": "devhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply eleven-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply eleven-db --remote",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "@opennextjs/cloudflare": "^1.14.3",
    "canvas-confetti": "^1.9.4",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.0",
    "jszip": "^3.10.1",
    "libsodium-wrappers": "^0.7.15",
    "next": "16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-sealedbox-js": "^1.2.0",
    "tweetnacl-util": "^0.15.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "better-sqlite3": "^12.5.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "tailwindcss": "^4",
    "wrangler": "^4.53.0"
  }
}
